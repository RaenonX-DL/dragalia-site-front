  "browserslist": {
    "production": [
      "> 0.2%",
      "not dead",
      "not op_mini all",
      "cover 99.5%"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
